<h1>Start the game</h1>

<h2><%= @new_grid %></h2>
<h3><%= @start_time %></h3>

<form action="/result" method="get">
  <input type="text" name="result">
  <%= hidden_field_tag :authenticity_token, form_authenticity_token %>
  <%= hidden_field_tag :new_grid, @new_grid %>
  
  <input type="submit" value="Submit !">
</form>

